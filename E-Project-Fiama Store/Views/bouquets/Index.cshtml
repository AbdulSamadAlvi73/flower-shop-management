@model IEnumerable<Fiama_Shop.Models.bouquet>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPageAdmin.cshtml";
}

<h2>Bouquets</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.bouqname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.bouqname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.price)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.bouqid }) |
            @Html.ActionLink("Details", "Details", new { id=item.bouqid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.bouqid })
        </td>
    </tr>
}

</table>
